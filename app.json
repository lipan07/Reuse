{
  "expo": {
    "name": "Shanidev",
    "slug": "Shanidev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/recycle.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/recycle.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/recycle.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lipan09.Shanidev"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/recycle.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a6800d68-3f0b-49a8-be6b-1ddd5b3422cc"
      }
    }
  }
}
